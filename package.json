{
  "name": "d8x-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "prettify": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "eslint": "eslint src --ext \".js,.jsx,.ts,.tsx\"",
    "eslint-fix": "eslint src --ext \".js,.jsx,.ts,.tsx\" --fix",
    "stylelint": "npx stylelint \"src/**/*.scss\" --color",
    "stylelint-fix": "npx stylelint \"src/**/*.scss\" --color --fix",
    "lint:staged": "lint-staged",
    "lint:ts": "npm run eslint",
    "lint:format": "prettier --check './{src,cypress}/**/*.{ts,tsx}'",
    "lint:format:fix": "prettier --write './{src,cypress}/**/*.{ts,tsx}'",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "npm run stylelint",
    "lint": "npm run lint:ts && npm run lint:css",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.scss": [
      "npm run stylelint"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run prettify",
      "npm run eslint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mui/icons-material": "5.11.0",
    "@mui/material": "5.11.6",
    "classnames": "2.3.2",
    "node-sass": "8.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-material-ui-carousel": "3.4.2",
    "react-router-dom": "6.7.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.4.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "eslint": "8.32.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "prettier": "2.8.3",
    "react-app-rewired": "2.2.1",
    "stylelint": "14.16.1",
    "stylelint-config-css-modules": "4.1.0",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-order": "6.0.1",
    "typescript": "4.9.4"
  }
}
